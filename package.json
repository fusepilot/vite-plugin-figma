{
  "name": "vite-plugin-figma",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fusepilot/vite-plugin-figma.git"
  },
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "npm run build && bumpp --commit --push --tag && npm publish",
    "postinstall": "tsup"
  },
  "devDependencies": {
    "@types/node": "^17.0.40",
    "bumpp": "^7.1.1",
    "esno": "^0.16.3",
    "tsup": "^6.1.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.9"
  }
}
